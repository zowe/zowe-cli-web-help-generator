            },
            {
              "name": "key",
              "description": "The command transaction key",
              "type": "string",
              "required": false,
              "group": "Options",
              "aliases": []
            },
            {
              "name": "ca7#",
              "description": "Defines the individual job number to hold. Omit if Q is specified.\n    jobnumber\nDefines the job number of the job to hold.\n    Limits:\n 1 to 4 numeric characters\n Required: Yes, unless the Q parameter is used.\n",
              "type": "string",
              "required": false,
