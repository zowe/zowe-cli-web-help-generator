            "optional": [
              "ca7",
              "base"
            ]
          },
          "options": [
            {
              "name": "ca7Id",
              "description": "CA7ONL Instance",
              "type": "string",
              "required": true,
              "group": "Required Options",
              "aliases": []
            },
            {
              "name": "job",
