      "type": "string",
      "group": "IDMS Connection Options",
      "required": false,
      "absenceImplications": null,
      "implies": null
    },
    {
      "name": "datasource",
      "aliases": [
        "d"
      ],
      "description": "Identifies the IDMS system where the API request will be sent and is defined in the data sources definition file for the IDMS REST API",
      "type": "string",
      "group": "IDMS Connection Options",
      "required": false,
      "absenceImplications": null,
      "implies": null
    },
    {
      "name": "base-path",
      "aliases": [
        "bp"
      ],
